@model List<SmsGateway.Application.CoreContext.DTO.SMSModule.AdministradorDTO>
@{
    ViewBag.Title = @Messages.website_Title;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>
   @* @ApplicationControlLayer.WebApplication.Shared.Traducao.Controller.Texto(9)*@</h2>
<p>
    @Html.ActionLink(@Messages.website_Create, "Create")
</p>
<table>
    <tr>
        <th>
          @Messages.website_Name
        </th>
        <th>
          @Messages.website_Email
        </th>
        <th>
            @Messages.website_IsEnabled
        </th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @(item.IsEnabled ? @Messages.Yes : @Messages.No)
            </td>
            <td>
                @Html.ActionLink(@Messages.website_Details, "Details", new { id = item.Id.ToString() })
            </td>
        </tr>
    }
</table>
